{
  "name": "tiktok-ext",
  "description": "A Vue.js web extension",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "tiktok-ext",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com https://www.tiktok.com; object-src 'self'",
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus",
    "notifications",
    "storage",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*",
    "unlimitedStorage",
    "management"
  ]
}